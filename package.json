{
    "name": "simply-foot",
    "version": "0.0.1",
    "main": "expo-router/entry",
    "scripts": {
        "start": "expo start",
        "start:android": "expo run:android",
        "start:ios": "expo run:ios",
        "web": "expo start --web",
        "build": "expo export",
        "build:web": "expo export -p web",
        "build:android": "expo export -p android",
        "build:ios": "expo export -p ios",
        "pack:android": "node scripts/build-android.js",
        "copy-apk": "node -e \"const pkg = require('./package.json'); const fs = require('fs'); const path = require('path'); const srcPath = path.join('android', 'app', 'build', 'outputs', 'apk', 'release', 'app-release.apk'); const destPath = `dist/simply-foot-v${pkg.version}.apk`; fs.copyFileSync(srcPath, destPath); console.log(`APK copied to ${destPath}`);\"",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "test:coverage": "jest --coverage",
        "lint": "expo lint",
        "lint:format": "expo lint --fix",
        "prettier": "prettier --check .",
        "prettier:format": "prettier --write ."
    },
    "jest": {
        "preset": "jest-expo",
        "testEnvironment": "jsdom"
    },
    "prettier": "./.prettierrc.json",
    "dependencies": {
        "@react-native-async-storage/async-storage": "2.1.2",
        "@react-native-community/datetimepicker": "^8.3.0",
        "@react-native-community/slider": "4.5.6",
        "@react-native-picker/picker": "2.11.0",
        "@react-navigation/drawer": "^7.3.9",
        "@react-navigation/native": "^7.1.6",
        "@supabase/supabase-js": "^2.49.4",
        "base-64": "^1.0.0",
        "base64-arraybuffer": "^1.0.2",
        "dayjs": "^1.11.13",
        "expo": "~53.0.9",
        "expo-av": "~15.1.6",
        "expo-camera": "~16.1.10",
        "expo-clipboard": "~7.1.4",
        "expo-dev-client": "~5.2.1",
        "expo-device": "~7.1.4",
        "expo-file-system": "~18.1.11",
        "expo-image-picker": "~16.1.4",
        "expo-linear-gradient": "~14.1.5",
        "expo-notifications": "~0.31.3",
        "expo-print": "~14.1.4",
        "expo-router": "~5.1.0",
        "expo-sharing": "~13.1.5",
        "expo-sqlite": "~15.2.12",
        "moti": "^0.30.0",
        "nativewind": "^4.1.23",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.4",
        "react-native-calendars": "^1.1313.0",
        "react-native-chart-kit": "^6.12.0",
        "react-native-gesture-handler": "~2.24.0",
        "react-native-reanimated": "~3.17.4",
        "react-native-safe-area-context": "5.4.0",
        "react-native-screens": "~4.11.1",
        "react-native-svg": "15.11.2",
        "react-native-toast-message": "^2.3.3",
        "react-native-web": "^0.20.0",
        "victory": "^37.3.6"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.23.0",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^30.0.0",
        "@types/react": "^19.1.8",
        "babel-plugin-module-resolver": "^5.0.2",
        "eslint": "^9.0.0",
        "eslint-config-expo": "~9.2.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "jest": "~29.7.0",
        "jest-expo": "~53.0.9",
        "metro": "^0.82.0",
        "metro-config": "^0.82.0",
        "metro-resolver": "^0.82.0",
        "prettier": "^3.6.2",
        "typescript": "^5.3.0"
    }
}
